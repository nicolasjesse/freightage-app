<%= form_with(url: search_prices_path, method: :get) do |form| %>
  <div>
    <%= form.label :height, 'Height (m)' %>
    <%= form.number_field :height, step: 0.01 %>
  </div>
  <div>
    <%= form.label :width, 'Width (m)' %>
    <%= form.number_field :width, step: 0.01 %>
  </div>
  <div>
    <%= form.label :depth, 'Depth (m)' %>
    <%= form.number_field :depth, step: 0.01 %>
  </div>
  <div>
    <%= form.label :weight, 'Weight (mg)' %>
    <%= form.number_field :weight, step: 0.01 %>
  </div>
  <div>
    <%= form.label :distance, 'Distance (KM)' %>
    <%= form.number_field :distance %>
  </div>
  <%= form.submit 'Search' %>
<% end %>